<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- ResourceDictionary identifier; used in App to discover the current localization -->
    <!-- Should get a "Localization.Comments="$Content(DoNotLocalize)"" attached... -->
    <sys:String x:Key="StringResourcesDictName">Loc-en-US</sys:String>
    
    <!-- Main window labels & load career button -->
    <sys:String x:Key="Server">Server:</sys:String>
    <sys:String x:Key="Locale">Locale:</sys:String>
    <sys:String x:Key="BattleTag">BattleTag:</sys:String>
    <sys:String x:Key="InvokeHeroes">Invoke my heroes</sys:String>
    
    <!-- Error messages -->
    <sys:String x:Key="errRetrievingProfile">Couldn't retrieve your profile. Please check your server and battletag.</sys:String>
</ResourceDictionary>