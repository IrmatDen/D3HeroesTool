<Application x:Class="D3HeroesTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d3htu="clr-namespace:D3HeroesTool.Utils"
             xmlns:d3d="clr-namespace:D3Data;assembly=D3Data"
             StartupUri="MainWindow.xaml" Startup="Application_Startup">
    <Application.Resources>
        <d3htu:ValidationErrorConverter x:Key="ValidationErrorConverter" />
        <d3htu:EnumDisplayer Type="{x:Type d3d:Server}" ResourcesPath="D3Data.resources" x:Key="ServersED" />
        <d3htu:EnumDisplayer Type="{x:Type d3d:Locale}" ResourcesPath="D3Data.resources" x:Key="LocalesED" />
    </Application.Resources>
</Application>
