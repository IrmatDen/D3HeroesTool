<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    
    <!-- ResourceDictionary identifier; used in App to discover the current localization -->
    <!-- Should get a "Localization.Comments="$Content(DoNotLocalize)"" attached... -->
    <sys:String x:Key="StringResourcesDictName">Loc-fr-FR</sys:String>
    
    <!-- Main window labels & load career button -->
    <sys:String x:Key="Server">Serveur :</sys:String>
    <sys:String x:Key="Locale">Langue :</sys:String>
    <sys:String x:Key="BattleTag">BattleTag :</sys:String>
    <sys:String x:Key="InvokeHeroes">Invoquer mes héros !</sys:String>
    
    <!-- Error messages -->
    <sys:String x:Key="errRetrievingProfile">Impossible de récupérer votre profile. Veuillez vérifier le serveur ainsi que votre BattleTag.</sys:String>
</ResourceDictionary>